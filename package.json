{
  "name": "userscript-starter-kit",
  "version": "1.0.0",
  "description": "A professional starter kit for building complex userscripts with a modern Gulp-based build process, supporting modular JS, CSS, and HTML, with multiple minification levels.",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp watch",
    "build": "gulp build",
    "build:dev": "gulp buildDev",
    "build:prod": "gulp buildProd",
    "build:aggressive": "cross-env AGGRESSIVE=true gulp buildProd",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "userscript",
    "violentmonkey",
    "tampermonkey",
    "greasemonkey",
    "starter-kit",
    "boilerplate",
    "gulp",
    "terser",
    "cssnano"
  ],
  "author": "Avitesh Murmu",
  "license": "MIT",
  "homepage": "https://github.com/Aviteshmurmu19/userscript-starter-kit#readme",
  "bugs": {
    "url": "https://github.com/Aviteshmurmu19/userscript-starter-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aviteshmurmu19/userscript-starter-kit.git"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.1",
    "cssnano-preset-advanced": "^7.0.0",
    "fs-extra": "^11.2.0",
    "gulp": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "html-minifier-terser": "^7.2.0",
    "terser": "^5.31.0"
  }
